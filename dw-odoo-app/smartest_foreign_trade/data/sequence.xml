<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="0">
        <!--
            The bellow sequence is used to auto generate Import File name
        -->
        <record id="import_file_lc_sequence" model="ir.sequence">
            <field name="name">Import File LC sequence</field>
            <field name="code">smartest.import.file</field>
            <field name="prefix">LC/%(year)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="import_file_tl_sequence" model="ir.sequence">
            <field name="name">Import File TL sequence</field>
            <field name="code">smartest.import.file</field>
            <field name="prefix">TL/%(year)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="import_file_withoutdom_sequence" model="ir.sequence">
            <field name="name">Import File Without DOM sequence</field>
            <field name="code">smartest.import.file</field>
            <field name="prefix">SD/%(year)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <!--
            The bellow sequence is used to auto generate Import Plan name
        -->
        <record id="import_plan_sequence" model="ir.sequence">
            <field name="name">Import Plan sequence</field>
            <field name="code">smartest.import.plan</field>
            <field name="prefix">Plan_Importation/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
       <!--
            The bellow sequence is used to auto generate Import Folder name
        -->
        <record id="import_folder_sequence" model="ir.sequence">
            <field name="name">Import Folder sequence</field>
            <field name="code">smartest.import.folder</field>
            <field name="prefix">IMPORT/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <!--
            The bellow sequence is used to auto generate export Folder name
        -->
        <record id="export_folder_sequence" model="ir.sequence">
            <field name="name">Export Folder sequence</field>
            <field name="code">smartest.export.folder</field>
            <field name="prefix">Export/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>