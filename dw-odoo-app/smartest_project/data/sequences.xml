<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_payment_ticket_id" model="ir.sequence">
            <field name="name">Sprint Sequence</field>
            <field name="code">project.sprint</field>
            <field name="prefix">SPRINT/%(y)sW%(woy)s/</field>
            <field name="padding">4</field>
        </record>
        <record id="sequence_project_team_id" model="ir.sequence">
            <field name="name">Teams Sequence</field>
            <field name="code">project.teams</field>
            <field name="prefix">TEAM/</field>
            <field name="padding">4</field>
        </record>
    </data>
</odoo>