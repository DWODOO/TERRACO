<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <!--
            The bellow Sequence is used to generate the Cash Ticket number Automatically.
        -->
        <record id="cash_ticket_sequence" model="ir.sequence">
            <field name="name">Cash Ticket Sequence</field>
            <field name="code">cash.ticket</field>
            <field name="prefix">TICKET/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="True"/>
        </record>

        <!--
            The bellow Sequence is used to generate the Cancel Ticket number Automatically.
        -->
        <record id="cash_ticket_cancel_sequence" model="ir.sequence">
            <field name="name">Cash Ticket Cancel Sequence</field>
            <field name="code">cash.ticket</field>
            <field name="prefix">CANCEL/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="True"/>
        </record>

        <!--
            The bellow Sequence is used to generate the Cash Ticket number Automatically.
        -->
        <record id="cash_register_sequence" model="ir.sequence">
            <field name="name">Cash Register Sequence</field>
            <field name="code">cash.register</field>
            <field name="prefix">CR/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="True"/>
        </record>
    </data>
</odoo>