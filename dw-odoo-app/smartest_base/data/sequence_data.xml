<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <!--
            The bellow sequence is used to auto generate partner code is the partner is a Supplier
        -->
        <record id="supplier_sequence" model="ir.sequence">
            <field name="name">Supplier sequence</field>
            <field name="code">res.partner.supplier</field>
            <field name="prefix">FR/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <!--
            The bellow sequence is used to auto generate partner code is the partner is a Customer
        -->
        <record id="customer_sequence" model="ir.sequence">
            <field name="name">Customer sequence</field>
            <field name="code">res.partner.customer</field>
            <field name="prefix">CL/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>