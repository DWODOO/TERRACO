<odoo>
<data>
    <record id="view_create_date_form_id" model="ir.ui.view">
            <field name="name">create.report.wizard.form</field>
            <field name="model">create.report.wizard</field>
            <field name="arch" type="xml">
                <form strin="date">
                    <group>
                        <field name="document" />
                        <field name="name" />
                        <field name="date" />

                    </group>
                    <footer>
                        <button name="init_doc" string="Print" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>


            </field>
    </record>

    <record id="action_create_date_id" model="ir.actions.act_window">
        <field name="name">Report</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">create.report.wizard</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="view_create_date_form_id"/>
        <field name="target">new</field>

    </record>
</data>

</odoo>