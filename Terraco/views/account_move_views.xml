<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="account_view_move_form_inherit_retourne" model="ir.ui.view">
        <field name="name">account_view_move_form_inherit</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='to_check']" position="after">
                <field name="restourne"/>
                <field name="delivery" string="delivery"  attrs="{'invisible': [('state', '!=', 'posted')]}"/>
            </xpath>
        </field>
    </record>

</odoo>
